public class save implements Serializable{
    private HashMap<String, Room> rMap = null;
    private ArrayList<Item> iMap = null;
    private ArrayList<Enemy> eList = null;
    private Inventory inv = null;
    private Parser par = null;
    private Room curRoom = null;
    private int health = 0;
    private boolean finish = false;

    public class save(HashMap<String, Room> Rmap, ArrayList <Item> Imap, ArrayList <Enemy> Elist, Inventory Inv, Parser Par, Room CurRoom, int Health, boolean finished){
        rMap = Rmap;
        iMap = Imap;
        eList = Elist;
        inv = Inv;
        par = Par;
        curRoom = CurRoom;
        health = Health;
        finish = finished;
    }
    public HashMap<String, Room> getRmap(){
        return rMap;
    }
    public ArrayList<Item> getImap(){
        return iMap;
    }
    public ArrayList<Enemy> getElist(){
        return eList;
    }
    public Inventory getInv(){
        return inv;
    }
    public Parser getPar(){
        return par;
    }
    public Room getCuRoom(){
        return curRoom;
    }
    public int getHealth(){
        return health;
    }
    public boolean getFin() {
        return finish;
    }
}